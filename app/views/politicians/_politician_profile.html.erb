
    <div id="main-container" class="container-fluid">
      <div class="row">
      <br>
      <br>
        <div id="sidebar" class="panel panel-default col-xs-3">
        <div class="panel body">
          <%= link_to image_tag("#{politician.image_name}"), politician_path(politician) %>
          </div>
        </div>
        <div id="content" class="panel panel-default col-xs-9">
        <div class="panel body">
          <h3><%= politician.full_name  %></h3>
          <p>Political Party: <%= politician.party %></p>
          <p>Prior Experience: <%= politician.prior_experience %></p>
          <p>Education: <%= politician.education %></p>
          <p>Email: <%= politician.email %></p>
          <p><%= politician.senate_or_house %></p>
        </div>
      </div>
    </div>
    </div>
  <br>
